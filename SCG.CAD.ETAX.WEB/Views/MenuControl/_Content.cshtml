@{
    Layout = null;
}


<script>


    $("#btn-CallPageProductUnit").click(function () {
        $.blockUI();

        $("#contentPartial").load("@Url.Action("Index", "ProductUnit")");

        $("#li-btnCallPageProductUnit").addClass("active");
        $("#li-btnCallPageTaxCode").removeClass("active");
        $("#li-btnCallPageRdDocument").removeClass("active");
        $("#li-btnCallPageDocumentCode").removeClass("active");


        $.unblockUI();
    });



    $("#btn-btnCallPageTaxCode").click(function () {
        $.blockUI();

        $("#contentPartial").load("@Url.Action("Index", "TaxCode")");

        $("#li-btnCallPageProductUnit").removeClass("active");
        $("#li-btnCallPageTaxCode").addClass("active");
        $("#li-btnCallPageRdDocument").removeClass("active");
        $("#li-btnCallPageDocumentCode").removeClass("active");



        $.unblockUI();
    });



    $("#btn-btnCallPageRdDocument").click(function () {
        $.blockUI();

        $("#contentPartial").load("@Url.Action("Index", "RdDocument")");

        $("#li-btnCallPageProductUnit").removeClass("active");
        $("#li-btnCallPageTaxCode").removeClass("active");
        $("#li-btnCallPageRdDocument").addClass("active");
        $("#li-btnCallPageDocumentCode").removeClass("active");

        $.unblockUI();
    });

    $("#btn-btnCallPageDocumentCode").click(function () {
        $.blockUI();

        $("#contentPartial").load("@Url.Action("Index", "DocumentCode")");

        $("#li-btnCallPageProductUnit").removeClass("active");
        $("#li-btnCallPageTaxCode").removeClass("active");
        $("#li-btnCallPageRdDocument").removeClass("active");
        $("#li-btnCallPageDocumentCode").addClass("active");


        $.unblockUI();
    });


</script>
