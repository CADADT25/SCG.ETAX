                    <i class="fas fa-file-export"></i> Export (.csv)
                </a>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped table-hover-animation table-hover" id="tbTransactionMain">
                        <thead>
                            <tr>
                                <th class="text-center">PK Key</th>
                                <th class="text-center">No</th>
                                <th>Billing No.</th>
                                <th>Billing Date</th>
                                <th>Billing Year</th>
                                <th>IsActive</th>
                                <th></th>
@*                                <th></th>

*@                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
