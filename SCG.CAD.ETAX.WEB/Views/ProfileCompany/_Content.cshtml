@{
    Layout = null;
}
@if (ViewBag.showVIEW == true)
{
    <h2 class="section-title"> Company Profile Info.</h2>

    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header d-flex justify-content-end align-items-center">
                    @if (ViewBag.showCREATE == true)
                    {
                        <a id="btnCallModalCreateProfileCompany" onclick="createProfileCompany();" class="pointer btn btn-icon icon-left text-white btn-primary">
                            <i class="fas fa-plus"></i> Create New
                        </a>
                    }
                    &nbsp;&nbsp;
                    @if (ViewBag.showEXPORT == true)
                    {
                        <a id="btnExportProfileCompany" onclick="ExportProfileCompany()" class="pointer btn btn-icon icon-left text-white btn-success">
                            <i class="fas fa-file-export"></i> Export (.csv)
                        </a>
                    }
                </div>


                <div class="card-body">



                    <div class="table-responsive">
                        <table class="table table-striped table-hover table-hover-animation" id="tbProfileCompanyMain">
                            <thead>
                                <tr>
                                    <th class="text-center">PK Key</th>
                                    <th class="text-center">No</th>
                                    <th>Code</th>
                                    <th>Name TH</th>
                                    <th>Name EN</th>
                                    @*<th>Certificate</th>*@
                                    <th>e-Bill</th>
                                    <th>Active</th>
                                    <th></th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
}




