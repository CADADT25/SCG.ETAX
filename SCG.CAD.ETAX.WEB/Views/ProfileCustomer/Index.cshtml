        var StatusEmail = ($('#toggleUpdateProfileCustomerEmail').prop('checked')) ? 1 : 0 ;
        var StatusSignPdf = ($('#toggleUpdateProfileCustomerFileSignedPdf').prop('checked')) ? 1 : 0 ;
