
<script src="~/assets/validate/ProfileReasonIssueValidate.js"></script>


<div class="row">
    <div class="col-md-12 col-12">
        <div class="form-group">
            <label>Data Source</label>
            <input autofocus disabled type="text" id="txtInsertProfileReasonIssueDataSource" onkeyup="validateInsertProfileReasonIssue();" class="form-control">
        </div>
    </div>


    <div class="col-md-12 col-12">
        <div class="form-group">
            <label>ERP Document Type</label>
            <input type="text" id="txtInsertProfileReasonIssueErpDocumentType" onkeyup="validateInsertProfileReasonIssue();" class="form-control">
        </div>
    </div>

    <div class="col-md-6 col-12">
        <div class="form-group">
            <label>ERP Reason Code</label>
            <input type="text" id="txtInsertProfileReasonIssueErpReasonCode" onkeyup="validateInsertProfileReasonIssue();" class="form-control">
        </div>
