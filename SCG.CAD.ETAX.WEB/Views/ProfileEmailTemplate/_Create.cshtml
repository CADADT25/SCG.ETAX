<form class="form form-vertical">
    <div class="row">


        <div class="col-12">
            <div class="form-group">
                <label><spa class="text-danger">* </spa>Type no.</label>
                <select class="select2 form-control" id="ddlInsertProfileEmailTemplateEmailTypeNo"></select>
            </div>
        </div>

        <div class="col-12">
            <div class="form-group">
                <label><spa class="text-danger">* </spa>Email body</label>
                <textarea id="txtInsertProfileEmailTemplateEmailBody" name="editordata"></textarea>
            </div>
        </div>

        <b>Setting object</b> : 

        <span class="pr-1"><p class="small" id="SUBJECT">[SUBJECT]</p></span> 
        <span class="pr-1"><p class="small" id="CUSTOMER-NAME">[CUSTOMER-NAME]</p></span>
        <span class="pr-1"><p class="small" id="CUSTOMER-ID">[CUSTOMER-ID]</p></span>
        <span class="pr-1"><p class="small" id="RD-DOCUMENT-NAME">[RD-DOCUMENT-NAME]</p></span>
        <span class="pr-1"><p class="small" id="DATE">[DATE]</p></span>
        <span class="pr-1"><p class="small" id="COMPANY-CODE">[COMPANY-CODE]</p></span>
        <span class="pr-1"><p class="small" id="COMPANY-NAME">[COMPANY-NAME]</p></span>
        <span class="pr-1"><p class="small" id="BILLING-NUMBER">[BILLING-NUMBER]</p></span>
        <span class="pr-1"><p class="small" id="LOGO">[LOGO]</p></span>
        <span class="pr-1"><p class="small" id="FILE-NAME">[FILE-NAME]</p></span>

        <div class="col-12 pt-5">
            <div class="form-group">
                <div class="custom-control custom-control-primary custom-checkbox">
                    <input type="checkbox" class="custom-control-input" id="toggleInsertProfileEmailTemplate" checked />
                    <label class="custom-control-label" for="toggleInsertProfileEmailTemplate">Is Active</label>
                </div>
            </div>
        </div>


    </div>
</form>

