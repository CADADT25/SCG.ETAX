<section id="faq-tabs">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="faq-payment" aria-labelledby="payment" aria-expanded="true">
                    <div class="collapse-margin collapse-icon mt-2" id="faq-payment-qna">

                        <div class="card">
                            <div class="card-header" id="paymentTwo" data-toggle="collapse" role="button" data-target="#faq-payment-two" aria-expanded="true" aria-controls="faq-payment-two">
                                <span class="lead collapse-title">Search</span>
                            </div>
                            <div id="faq-payment-two" class="collapse show" aria-labelledby="paymentTwo" data-parent="#faq-payment-qna">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Customer Code</label>
                                                <select class="select2 form-control" id="OutputSearchEmailSendCustomerCode" multiple>
                                                    <option value="All">-- All -- </option>
                                                </select>
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Send date </label>
                                                <input type="text" id="OutputSearchEmailSendDate" class="form-control flatpickr-date-time" placeholder="DD-MM-YYYY">
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Download status</label>
                                                <select class="select2 form-control" id="OutputSearchEmailSendStatus">
                                                    <option value="All">-- All -- </option>
                                                    <option value="SENT">-- SENT -- </option>
                                                    <option value="NOTSEND">-- No send -- </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>