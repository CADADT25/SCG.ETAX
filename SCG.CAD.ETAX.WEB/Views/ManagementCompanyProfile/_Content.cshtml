@{
    Layout = null;
}

<section id="basic-tabs-components">
    <div class="row match-height">

        <!-- Tabs with Icon starts -->
        <div class="col-xl-12 col-lg-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Company Profile Management</h4>
                </div>
                <div class="card-body">
                    <ul class="nav nav-tabs" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="DocumentCode-tab" data-toggle="tab" href="#DocumentCodeTab" aria-controls="profile" role="tab" aria-selected="true">
                                <i class="fas fa-user"></i> Document Code
                            </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link" id="IssueReasonDocumentCode-tab" data-toggle="tab" href="#IssueReasonDocumentCodeTab" aria-controls="profile" role="tab" aria-selected="false">
                                <i class="fas fa-user"></i> Issue Reason Code
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" id="ProductUnit-tab" data-toggle="tab" href="#ProductUnitTab" aria-controls="profile" role="tab" aria-selected="false">
                                <i class="fas fa-user"></i> Product Unit
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" id="TaxCode-tab" data-toggle="tab" href="#TaxCodeTab" aria-controls="profile" role="tab" aria-selected="false">
                                <i class="fas fa-user"></i> Tax Code
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" id="SellOrg-tab" data-toggle="tab" href="#SellOrgTab" aria-controls="profile" role="tab" aria-selected="false">
                                <i class="fas fa-user"></i> Sell Org
                            </a>
                        </li>

                         <li class="nav-item">
                            <a class="nav-link" id="SellerAddress-tab" data-toggle="tab" href="#SellerAddressTab" aria-controls="profile" role="tab" aria-selected="false">
                                <i class="fas fa-user"></i> Seller Address
                            </a>
                        </li>

                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="DocumentCodeTab" aria-labelledby="DocumentCode-tab" role="tabpanel">
                            @await Html.PartialAsync("~/Views/DocumentCode/Index.cshtml")
                        </div>
                        <div class="tab-pane" id="IssueReasonDocumentCodeTab" aria-labelledby="profileIcon-tab" role="tabpanel">
                            @await Html.PartialAsync("~/Views/ProfileReasonIssue/Index.cshtml")
                        </div>
                        <div class="tab-pane" id="ProductUnitTab" aria-labelledby="disabledIcon-tab" role="tabpanel">
                            @await Html.PartialAsync("~/Views/ProductUnit/Index.cshtml")
                        </div>
                        <div class="tab-pane" id="TaxCodeTab" aria-labelledby="aboutIcon-tab" role="tabpanel">
                            @await Html.PartialAsync("~/Views/TaxCode/Index.cshtml")
                        </div>
                        <div class="tab-pane" id="SellOrgTab" aria-labelledby="aboutIcon-tab" role="tabpanel">
                            @await Html.PartialAsync("~/Views/ProfileSellOrg/Index.cshtml")
                        </div>
                        <div class="tab-pane" id="SellerAddressTab" aria-labelledby="aboutIcon-tab" role="tabpanel">
                            @await Html.PartialAsync("~/Views/ProfileSeller/Index.cshtml")
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Tabs with Icon ends -->
    </div>
</section>