                    <h4 class="card-title"> Sell Org </h4>
                </div>
                <div class="card-body" id="render-profileSellOrg-update">
                    @await Html.PartialAsync("~/Views/ProfileSellOrg/Index.cshtml")
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Basic Floating Label Form section end -->

<!-- Basic multiple Column Form section start -->
<section id="section-profileCertificate-create">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title"> Certificate </h4>
                </div>
                <div class="card-body" id="render-profileCertificate-update">
