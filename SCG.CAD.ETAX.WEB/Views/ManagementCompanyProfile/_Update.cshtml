@{
    Layout = null;
}


<div class="content-header row">
    <div class="content-header-left col-md-9 col-12 mb-2">
        <div class="row breadcrumbs-top">
            <div class="col-12">
                <h2 class="content-header-title float-left mb-0">Company Profile Update</h2>
                <div class="breadcrumb-wrapper">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a class="text-primary" onclick="#">Home</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a class="text-primary" onclick="callProfileCompanyIndex();">Company Profile</a>
                        </li>
                        <li class="breadcrumb-item active">
                            <a onclick="javascript:void(0);">Update</a> :
                            <label id="companyCodeStateUpdate"></label>
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Basic multiple Column Form section start -->
<section id="section-ProfileCompany-create">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title"> Company Info</h4>
                </div>
                <div class="card-body">
                   @await Html.PartialAsync("~/Views/ProfileCompany/_Update.cshtml")
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Basic Floating Label Form section end -->

<!-- Basic multiple Column Form section start -->
<section id="section-profileSeller-create">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title"> Company Address</h4>
                </div>
                <div class="card-body" id="render-profileSeller-update">
                    @await Html.PartialAsync("~/Views/ProfileSeller/Index.cshtml")
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Basic Floating Label Form section end -->



<!-- Basic multiple Column Form section start -->
<section id="section-profileSellOrg-create">
