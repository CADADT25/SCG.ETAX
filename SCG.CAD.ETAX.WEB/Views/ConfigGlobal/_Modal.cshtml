@{
    Layout = null;
}

<script src="~/assets/validate/ConfigGlobalValidate.js"></script>


<div class="modal fade" tabindex="-1" role="dialog" id="modalCreateConfigGlobal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Create ConfigGlobal</h5>
                <button type="button" class="close" onclick="fnInsertCancelConfigGlobal();" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

                @await Html.PartialAsync("~/Views/ConfigGlobal/_Create.cshtml")

            </div>
            <div class="modal-footer bg-whitesmoke br">
                <button type="button" class="btn btn-secondary" onclick="fnInsertCancelConfigGlobal();">Close</button>
                <button type="button" class="btn btn-primary" onclick="fnInsertConfigGlobal();">Save changes</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" tabindex="-1" role="dialog" id="modalUpdateConfigGlobal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Update ConfigGlobal</h5>
                <button type="button" class="close" onclick="fnUpdateCancelConfigGlobal();" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

                @await Html.PartialAsync("~/Views/ConfigGlobal/_Update.cshtml")

            </div>
            <div class="modal-footer bg-whitesmoke br">
                <button type="button" class="btn btn-secondary" onclick="fnUpdateCancelConfigGlobal();">Close</button>
                <button type="button" class="btn btn-primary" onclick="fnUpdateConfigGlobal();">Save changes</button>
            </div>
        </div>
    </div>
</div>
